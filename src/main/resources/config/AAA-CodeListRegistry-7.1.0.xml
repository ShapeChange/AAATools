			<targetParameter name="config" value="GeoInfoDok"/>
			<targetParameter name="datetimePublished" value="2009-05-31T10:00:00Z"/>
			<targetParameter name="datetimeUpdated" value="2018-07-31T00:00:00Z"/>
			<targetParameter name="referenceModelType" value="EA7"/>
			<targetParameter name="referenceModelFile" value="AAA-6.0.1.eap"/>
		</Target>
	</targets>
</ShapeChangeConfiguration>